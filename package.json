{
	"name": "kupaliska-starz-be",
	"version": "0.0.1",
	"description": "API interface for Kupaliska Bratislava",
	"scripts": {
		"build": "rimraf ./dist && tsc --project tsconfig.json",
		"build:db": "rimraf ./dist && tsc --project tsconfig.database.json",
		"debug": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true NODE_ENV=development nodemon --watch src --ext 'js,ts,json' -L --exec 'node --inspect -r dotenv/config -r ts-node/register src/index.ts' | pino-pretty -c",
		"start": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node -r dotenv/config -r ts-node/register src/index.ts",
		"start:workers": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node -r dotenv/config -r ts-node/register src/workers/index.ts",
		"script": "cross-env TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true node -r dotenv/config -r ts-node/register",
		"test": "cross-env DOTENV_CONFIG_PATH=.env.test NODE_ENV=test JWT_SECRET=jwtrandomsecret npm run test:real",
		"test:coverage": "cross-env  DOTENV_CONFIG_PATH=.env.test NODE_ENV=test  JWT_SECRET=jwtrandomsecret npm run test:coverage:real",
		"test:real": "npm run clean && node -r dotenv/config --expose-gc \"./node_modules/jest/bin/jest.js\" --runInBand --passWithNoTests --logHeapUsage",
		"test:coverage:real": "npm run clean && node -r dotenv/config --expose-gc \"./node_modules/jest/bin/jest.js\" --runInBand --passWithNoTests --coverage",
		"clean": "npm run build:db && npm run migrate:undo:all && npm run migrate",
		"lint": "esw src tests --color --ext .js --ext .ts",
		"lint:fix": "esw src tests --color --ext .js --ext .ts --fix",
		"lint:watch": "npm run lint -- --watch  --ext .js --ext .ts",
		"apidoc:scan": "apidoc -i src/ -o apidoc/ -f \"^.{1,}(.apidoc)$\"",
		"translate:scan": "i18next-scanner 'src/**/*.ts'",
		"migrate": "./node_modules/.bin/sequelize db:migrate --config \"./dist/config/database.js\" --migrations-path \"./dist/src/db/migrations\"",
		"migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo --config \"./dist/config/database.js\" --migrations-path \"./dist/src/db/migrations\"",
		"migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all --config \"./dist/config/database.js\" --migrations-path \"./dist/src/db/migrations\"",
		"migrate:dev": "npm run build:db && \"./node_modules/.bin/sequelize\" db:migrate --config \"./dist/config/database.js\" --migrations-path \"./dist/src/db/migrations\"",
		"migrate:dev:undo": "npm run build:db && \"./node_modules/.bin/sequelize\" db:migrate:undo --config \"./dist/config/database.js\" --migrations-path \"./dist/src/db/migrations\"",
		"migrate:dev:undo:all": "npm run build:db && \"./node_modules/.bin/sequelize\" db:migrate:undo:all --config \"./dist/config/database.js\" --migrations-path \"./dist/src/db/migrations\"",
		"seed:prod": "./node_modules/.bin/sequelize db:seed:all --config \"./dist/config/database.js\" --seeders-path \"./dist/src/db/seeders/production\"",
		"seed:undo:prod": "./node_modules/.bin/sequelize db:seed:undo --config \"./dist/config/database.js\" --seeders-path \"./dist/src/db/seeders/production\"",
		"seed:undo:all:prod": "./node_modules/.bin/sequelize db:seed:undo:all --config \"./dist/config/database.js\" --seeders-path \"./dist/src/db/seeders/production\"",
		"seed:dev": "npm run build:db && \"./node_modules/.bin/sequelize\" db:seed:all --config \"./dist/config/database.js\" --seeders-path \"./dist/src/db/seeders/development\"",
		"seed:undo:dev": "npm run build:db && \"./node_modules/.bin/sequelize\" db:seed:undo --config \"./dist/config/database.js\" --seeders-path \"./dist/src/db/seeders/development\"",
		"seed:undo:all:dev": "npm run build:db && \"./node_modules/.bin/sequelize\" db:seed:undo:all --config \"./dist/config/database.js\" --seeders-path \"./dist/src/db/seeders/development\""
	},
